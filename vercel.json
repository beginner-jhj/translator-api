{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/translate",
      "dest": "/api/index.js",
      "methods":["POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type:":"application/json"
      }
    }
  ]
}
